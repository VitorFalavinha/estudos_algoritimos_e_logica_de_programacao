Algoritmo "teste_para"

Var

i, n, soma, quant : inteiro

Inicio

    escreva("Quantos numeros serão digitados? ")
    leia(quant)
    
    soma <- 0
    
    // para i de 1 ate i faca    => POSSO FAZER ISSO????
    
    para i de 1 ate quant faca
         escreva("Digite um numero: ")
         leia(n)
         soma <- soma + n
    fimpara
    
    escreva("Soma = ", soma)
         

Fimalgoritmo